{
  "_id": "global-classification",
  "type": "pipe",
  "source": {
    "type": "merge",
    "datasets": ["brreg-enhetsregisteret-classification bc", "nace-code-enrich nc", "twelvedata-exchange-enrich te", "dbpedia-industry-enrich di", "dbpedia-type-enrich dt", "hubspot-company-classification hcce", "dbpedia-country-enrich dc"],
    "equality_sets": [
      ["nc.dct:identifier",
        ["substring", 0, 5, "bc.brreg-enhetsregisteret:value"]
      ],
      [
        ["lower", "dc.rdfs:label"],
        ["lower", "hcce.hubspot-company-country:value"],
        ["lower", "bc.brreg-enhetsregisteret-land:value"]
      ]
    ],
    "identity": "first",
    "max_merged": 100,
    "strategy": "compact",
    "version": 2
  },
  "sink": {
    "indexes": "$ids"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["add", "value",
          ["first",
            ["coalesce",
              ["list", "_S.skos:prefLabel", "_S.dct:title", "_S.value", "_S.name", "_S.rdfs:label", "No value"]
            ]
          ]
        ]
      ]
    }
  },
  "metadata": {
    "global": true
  }
}
